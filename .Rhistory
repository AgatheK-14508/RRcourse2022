#2.This function reverses a string
reverse_chars <- function(string)
{
string_split =
strsplit(as.character(string), split = "")
reversed_split =
string_split[[1]][nchar(string):1]
paste(reversed_split, collapse = "")
}
reverse_chars(9126)
